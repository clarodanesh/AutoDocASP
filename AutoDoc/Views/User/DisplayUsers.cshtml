
@{
    ViewBag.Title = "DisplayUsers";
}

<h2>DisplayUsers</h2>
<table>
    <tr>
        <th>
            Username
        </th>
        <th>
            Email
        </th>
    </tr>
    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.Encode(item.firstname)
        </td>
        <td>
            @Html.Encode(item.email)
        </td>
    </tr>
    }
</table>

